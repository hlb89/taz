<script>
$(function() {

	var initiateAnimation = function() {
		$("div#animation").sequencer({
			count: 22,
			path: "./assets/images/animations/",
			ext: "png"
		}, function() {
			$("div#preload").hide();
		});

		var getHeight = $(document).height();
		$("div#animation").css("height", getHeight);
	}

	if ($(window).width() > 767) {
		initiateAnimation();
	} else {
		$("div#preload").hide();
	}



})
</script>


